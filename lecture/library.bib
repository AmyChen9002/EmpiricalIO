}

@article{Matzkin1994b,
	title = {Chapter 42 {Restrictions} of economic theory in nonparametric methods},
	volume = {4},
	issn = {1573-4412},
	url = {https://www.sciencedirect.com/science/article/pii/S157344120580011X},
